    <script src="~/Content/Plugins/nanobar/nanobar.min.js"></script>
    <script src="~/Content/Plugins/Toastr/toastr.min.js"></script>
    <script src="~/Content/Scripts/Script.js"></script>
    <script>
        var nanobar = new Nanobar({ bg: '#ffeca8' });

        $(function () {
            GetCheckInDetails();
            GetAnniversary();
        });

        function BtnCheckClick() {
            var confirmation;
            if($('#CheckIn').text() == "Check Out")
                confirmation=confirm("Are you sure you want to checkout?");
            else
            confirmation=true;

            if(confirmation)
            {
                $.ajax({
                        url: "@(Url.Action("InsertCheckInDetails", "Home"))",
                        type: "Post",
                        data: {},
                        dataType: "json",
                        success: function (data) {
                            GetCheckInDetails();
                        },
                        error: function (xhr, ajaxOptions, thrownError) {
                            showToaster("Error getting data", "error");
                        }
                    });
            }
        }

        function GetCheckInDetails() {
            $.ajax({
                url: "@(Url.Action("GetCheckInDetails", "Home"))",
                type: "Post",
                data: {},
                dataType: "json",
                success: function (data) {
                    if (data.length > 0) {
                        //var CheckOut = data[0].OutTime.Hours + ':' + data[0].OutTime.Minutes;
                        //var CheckIn = data[0].InTime.Hours + ':' + data[0].InTime.Minutes;

                        $('#CheckIn').show();

                        if (data[0].InTime == null || (data[0].InTime != null && data[0].InTime != "00:00")) {
                            $('#lblmessage1').html('<i class="fa fa-hourglass-start"></i>  ' + data[0].InTime).show();
                            $('#CheckIn').text("Check Out");
                        }

                        if ($('#CheckIn').text() == "Check Out" && data[0].OutTime != "00:00" && data[0].OutTime != null) {
                            $('#lblmessage1').html($('#lblmessage1').html() + '<br/><i class="fa fa-hourglass-end"></i>  ' + data[0].OutTime)
                                .show().removeClass("text-checkin");
                            $('#CheckIn').hide();
                        }
                    }
                    else
                        $('#CheckIn').show();
                }
            });
        }

        function FormatTime(time)
        {
            var hours = time.Hours;
            var minutes = time.Minutes;
            var ampm = hours >= 12 ? 'pm' : 'am';
            hours = hours % 12;
            hours = hours ? hours : 12;
            minutes = minutes < 10 ? '0' + minutes : minutes;
            return hours + ':' + minutes + ' ' + ampm;
        }

        function GetHolidaysDetails() {

            nanobar.go(30);
            $.ajax({
                cache: 'false',
                method: 'POST',
                url: '@Url.Action("HolidayDetails", "Holidays")',
                contentType: 'application/json',
                data: {},
                dataType: 'JSON',
                success: function (data) {
                    $("#tbGrid1 tbody").empty();
                    for (var i = 0; i < data.length; i++) {
                        var tr;
                        if (i % 2 != 0) {
                            tr = $('<tr class="info"/>');
                        }
                        else {
                            tr = $('<tr/>');
                        }
                        td = $('<td/>');
                        tr.append("<td>" + "<span class='far fa-file-alt'></span>&nbsp;&nbsp;&nbsp;" + data[i].Year + "</td>");
                        tr.append("<td>" + data[i].Date + "</td>");
                        tr.append("<td>" + data[i].Day + "</td>");
                        tr.append("<td>" + data[i].Festival + "</td>");

                        $('#tbGrid1').append(tr);
                    }
                    nanobar.go(100);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(xhr.status);
                }
            });
        }


        var dat1 = "";
        function GetCheckOutDetails() {
            $.ajax({
                url: "@(Url.Action("GetCheckOutDetails", "Home"))",
                type: "Post",
                data: {},
                dataType: "json",
                success: function (data) {
                    if (data.length > 0) {
                        dat1 = moment(data[0].Date).format('MM/DD/YYYY');
                        var CheckOut = data[0].OutTime.Hours + ':' + data[0].OutTime.Minutes;
                        var CheckIn = data[0].InTime.Hours + ':' + data[0].InTime.Minutes;

                        if (data[0].InTime != null || CheckIn != "0:0") {
                            if (data[0].OutTime == null || CheckOut == "0:0") {
                                $("#myModalCheckOut").modal('show');
                                $('#lblMessageDate').html("You didn't Check Out on <b>" + moment(data[0].Date).format('MM/DD/YYYY'));
                            }
                        }

                    }
                }

            });
        }

    function UpdateCheckOutTime() {

            nanobar.go(30);
            var OutTime = $('#OutTime').val();
            var name = $('#btnTimeSave').text();
            if (name == "Save") {
                $.ajax({

                    url: "@(Url.Action("UpdateCheckOutDetails", "Home"))",
                    type: 'POST',
                    dataType: 'json',
                    data: { "OutTime": OutTime, "Date":dat1 },
                    success: function (data) {

                        console.log(data);
                        nanobar.go(60);

                        if (data != null && typeof data != "object") data = JSON.parse(data);
                        if (data.Valid == "false") {
                            $(data.Data).each(function (i, e) {
                                $('#' + e.key).addClass('input-validation-error');
                                $("span[data-valmsg-for='" + e.key + "'").removeClass('field-validation-valid').addClass('field-validation-error').html(e.errors[0]);
                            });
                        }
                        else if (data.Valid == "true") {
                            $('#OutTime').val('');
                            if (data.Success == "false") {
                                if (data.Data != "SessionExpired") toastr.error("", data.Data);
                                else location.href = "@Url.Action("Index", "Login")";
                            }
                            else {
                                if (data.Success == "true") {
                                    toastr.success("", data.Data);
                                    setTimeout(function () {
                                        location.href = "@Url.Action("Index", "Home")";
                                    }, 1000);
                                }
                            }
                        }
                        nanobar.go(100);
                    },
                    error: function (x, e, data) {
                        if (x.status == 401) {
                            nanobar.go(100);
                            location.href = "@Url.Action("Timeout", "Account")";
                        }
                    }
                });
            }
        }
         function GetAnniversary() {

               nanobar = new Nanobar({ bg: '#367fa9' });
                nanobar.go(30);
                $.ajax({
                    cache: 'false',
                    method: 'POST',
                    url: '@Url.Action("EmployeeAnniversary", "Home")',
                    contentType: 'application/json',
                    data: '',
                    dataType: 'JSON',
                    success: function (data) {
                        if (data.length > 0) {
                            $('#anniversary').show();
                            for (var i = 0; i < data.length; i++) {
                            if (data[i].ISDOA==1)
                                $("#wishes").append('<li><a href="#"> <i class="fa fa-heart" style="color:Red"></i> ' + 'Happy Wedding Anniversary ' + data[i].FirstName + '</a></li>');
                            if (data[i].ISDOB == 1)
                                $("#wishes").append('<li><a href="#"> <i class="fa fa-birthday-cake" style="color:violet"></i>' + 'Happy Birthday ' + data[i].FirstName + '</a></li>');
                            if (data[i].ISDOJ == 1)
                                $("#wishes").append('<li><a href="#"><i class="fa fa-briefcase"></i> ' + 'Happy work Anniversary ' + data[i].FirstName + '</a></li>');
                            }
                        }
                        nanobar.go(100);
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        alert(xhr.status);
                    }
                });

        }

        function MissingEntryDetails() {
            var nanobar = new Nanobar({ bg: '#367fa9' });
            nanobar.go(30);
            var st=1;
            var model = {
                "EmployeeID": @Session["EmployeeId"],
                };
            var form_data = new FormData();
            for (var key in model) {
                form_data.append(key, model[key]);
            }
            $.ajax({
                cache: 'false',
                method: 'POST',
                url: '@Url.Action("MissingEntriesDetails", "Reports")',
                contentType: 'application/json',
                data: JSON.stringify({ model:model,st:st}), //use id here
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                   var j = 0;
                   $('#Count').append(data.length);
                   if( data.length>0)
                   {
                       $('#Bell').show();
                   }
                    for (var i = 0; i < data.length; i++) {
                        $('#missing').append('<li><a href="@Url.Action("Missing", "Reports", new { @id = ('1') })"><i class=""></i>'+ data[i].Date + ' ' + data[i].InTime+' '+data[i].OutTime+'</li>');
                        if(i > 9) return false;
                    }
                    nanobar.go(100);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(xhr.status);
                }
            });
        }
    </script>
    @RenderSection("scripts", required: false)
</body>
